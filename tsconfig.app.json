{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"lib": ["es6"],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*.ts", "src/**/*.d.ts", "app/**/*.d.ts"],
	"exclude": ["node_modules"]
}
