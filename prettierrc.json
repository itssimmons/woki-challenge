{
	"$schema": "https://json.schemastore.org/prettierrc",
	"semi": true,
	"trailingComma": "none",
	"singleQuote": true,
	"plugins": ["@ianvs/prettier-plugin-sort-imports"],
	"importOrder": [
		"<BUILTIN_MODULES>",
		"<THIRD_PARTY_MODULES>",
		"",
		"<TYPES>",
		"^[.]"
	],
	"importOrderSeparation": true
}
