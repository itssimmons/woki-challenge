		"@ianvs/prettier-plugin-sort-imports": "^4.7.0",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.10.0",
		"esbuild": "^0.25.12",
		"eslint": "^9.39.1",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"jest": "^30.2.0",
		"jiti": "^2.6.1",
		"lint-staged": "^16.2.7",
